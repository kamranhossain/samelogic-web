  "homepage": "https://github.com/Samelogic/samelogic-web#readme",
  "devDependencies": {
    "babel-jest": "^12.0.2",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.5.1",
    "eslint-plugin-meteor": "^3.4.0",
    "eslint-plugin-react": "^4.2.3",
